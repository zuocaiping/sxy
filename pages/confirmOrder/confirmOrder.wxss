/* pages/list/list.wxss */
page{padding-bottom: 260rpx;background: #ebeeef;}
.m-top{position: relative;padding-left:39rpx;margin-bottom:11rpx;line-height: 58rpx;background: #fff; }
.m-top .m-left{font-size: 36rpx;color: #242424; }
.m-top .m-left image{width:18rpx;height:32rpx;margin: 14rpx 10rpx 0 0; vertical-align: top;}
.m-block2{position: relative;}
.m-block2 .m-list{padding-top: 30rpx;}
.m-block2 .u-info{position: absolute;left: 0;top:-10rpx;color: #717171;font-size: 26rpx;}
.m-list label{float: left;font-size: 28rpx;color: #999999;}
.m-list label text{float: none;}
.m-list label checkbox {transform:scale(.6);margin-top:-5rpx;margin-left:-5rpx;}
.m-bc{background: #fff;border-top:20rpx solid #ebeeef;overflow: hidden;}
.m-rule{width: 30rpx;height: 30rpx; display: inline-block;background: #a6a6a6;border-radius: 50%; line-height: 30rpx;text-align: center;color: #fff;margin-left: 15rpx;font-size: 28rpx;margin-top: 35rpx;vertical-align: top;}
.m-list {border-bottom: 1px solid #f0f0f0;line-height: 98rpx;height: 98rpx; padding-right:54rpx;margin-left:38rpx; position: relative; }
.m-list:last-child{border-bottom: none;}
.m-list .u-green{color: #52b87d;float: right;}
.m-list text{ float: left; font-size: 30rpx; color: #333333; line-height: 98rpx;}
.m-list image{width: 11rpx;height: 22rpx; position: absolute;right: 30rpx;top: 50%;margin-top: -11rpx;}
.m-list .u-red{color: #fe3131;font-size: 22rpx;margin-left: 19rpx;}
.m-list .u-paper{font-size: 24rpx; color: #cccccc;margin-left: 19rpx;}
.m-list .u-color{color: #953f3f; display: inline-block;}
.m-list .u-price1{float: right;font-size: 24rpx;}
.m-list .u-price1 text{color: #333333;float: right;font-size: 30rpx;}
.m-list ._list{color: #4266b0;display: inline-block;text-decoration: underline;}
.m-address{position: relative;padding: 20rpx 0 30rpx;background: #fff;}
.m-address image{width: 11rpx;height: 22rpx; position: absolute;right: 30rpx;top: 50%;margin-top: -11rpx;}
.m-address .address{font-size: 26rpx;color: #6c6c6c;margin: 0 0 0 38rpx;width: 80%;overflow: hidden;white-space: nowrap;text-overflow: ellipsis;margin-bottom:5rpx;}
.m-address .address text{font-weight: bold;color: #333333;}
.m-address .phone{font-size: 26rpx;color: #797979;margin-left:140rpx; }
.letter-box{
        width: 278px;
        height: 176px;
        padding: 5px;
        box-sizing: border-box;
    }
    .letter-border{
        width: 100%;
        height: 100%;
        background: #fbfaf5;
    }
    .letter-box{
        width: 100%;
        height: 6rpx;
        padding: 6rpx;
        box-sizing: border-box;
        background: linear-gradient(45deg,#5acc82 12.5%,#ffffff 12.5%,#ffffff 25%,#52abe9 25%,#52abe9 37.5%,#ffffff 37.5%,#ffffff 50%,#5acc82 50%,#5acc82 62.5%,#ffffff 62.5%,#ffffff 75%,#52abe9 75%,#52abe9 87.5%,#ffffff 87.5%,#ffffff 100%);
        background-size: 70px 70px;
    }
.m-data{border-top:20rpx solid #ebeeef;border-bottom:20rpx solid #ebeeef;background: #fff;}
.m-recommend-con{ position: relative; display: flex;padding: 30rpx 38rpx 20rpx;}
.m-comment{font-size: 24rpx; color: #666666; line-height: 32rpx;border-top:1px solid #f0f0f0;padding-top:26rpx; position: relative;}
.m-comment image{width: 32rpx;height: 31rpx;margin: 5rpx 10rpx 0 0; vertical-align: top; position: absolute;}
.m-comment view{margin-left:20rpx; display: inline-block; }
.m-comment text{color: #519b61;margin-left: 10rpx;}
.m-con{flex: 1;margin-right: 90rpx;}
.m-con text{display: block;}
.u-name{font-size: 32rpx; color: #333;margin-bottom: 12rpx; line-height: 40rpx;}
.u-price{position: absolute;font-size: 22rpx; color: #262626;right: 35rpx;top:28rpx;}
.m-book{background: #ebebeb;border-radius: 20rpx;width:130rpx;height: 122rpx; float: left;margin:0 30rpx 0 0rpx; position: relative; }
.m-book image{width: 70rpx;height: 109rpx; margin: 10rpx auto 0; display: block;}
.m-book text{width:50rpx;height: 50rpx;border-radius: 50%;text-align: center;line-height: 50rpx;font-size: 16rpx;color: #fff; position: absolute;right:24rpx;top:-25rpx; display: block;background: #fdb73c;}
.u-about{font-size: 22rpx; line-height: 32rpx;color: #b0afaf;margin-bottom: 20rpx;}
.m-text{border-top: 1px solid #f0f0f0; display: flex;margin: 0 40rpx;}
.m-text text{flex: 1;font-size: 30rpx; color: #333333;height: 76rpx; line-height: 76rpx;}
.m-text text.u-right{text-align: right;}
.m-radio{font-size: 28rpx; color: #999999;margin: 34rpx 0 30rpx 30rpx;}
.m-radio text{ color: #333333;}
.m-radio checkbox{transform:scale(.6);margin-top:-5rpx;}
.m-radio navigator{display: inline-block;}
.m-fun{overflow: hidden;position: fixed;height:138rpx;bottom:0;left: 0;width: 100%;background: #fff;box-sizing: border-box;padding: 20rpx 0;}
.m-fun .u-text{font-size:24rpx;margin-left:40rpx;color: #333333;line-height: 98rpx;margin-right: 38rpx;}
.m-fun .m-btn{width: 270rpx;height: 98rpx;margin-right: 30rpx; background: #5acc82;color: #fff;font-size: 30rpx; line-height: 98rpx; text-align: center;font-weight: bold;float: right;border-radius: 60rpx;}
.m-fun text{color: #fe3131;font-weight: bold;font-size:30rpx;}
.m-address-text{background: #fff1b0;height:70rpx;line-height: 70rpx;font-size: 26rpx;color: #181818;position:fixed;bottom: 138rpx;width: 100%;padding: 10rpx 20rpx;overflow:hidden;    text-overflow:ellipsis;    white-space:nowrap}


.modal-box{
  position:fixed;
  width:100%;
  height:100%;
  top:0%;
  background:rgba(0,0,0,0.4);
  overflow: hidden;
  }

.modal-body{
  position:fixed;width: 100%;box-sizing: border-box;
  background: #fff;z-index: 100;bottom:0;padding: 0 30rpx;
  }

.modal-content{
  /* width: 60%; */
  overflow: hidden;}

.modal-content{
  width: 100%;
}
.modal-content .fenxiang{
  width: 100%;
  height: 40%;
}
.modal-title{border-bottom: 1px solid #f2f2f2;text-align: center; display: flex;line-height: 100rpx;padding:0 10rpx;font-size: 34rpx;}
.modal-title .u-sure{text-align: right;color: #2ebb3b;font-size: 28rpx;}
.modal-title .u-cancel{text-align: left;color: #a0a0a0;font-size: 28rpx;}
.modal-title view{display: inline-block;flex: 1;}
.modal-list{font-size: 34rpx;color: #333333;padding:20rpx 0 20rpx 20rpx;}
.modal-list text{color: #b9b9b9;font-size: 28rpx;margin-left: 10rpx;}
.modal-list checkbox {transform:scale(.6);margin-top:-5rpx;float: right;}
/*步知币规则*/
.rule-body{border-radius:15rpx;width:90%;left:50%;bottom:30%;margin-left: -45%;}
.rule-body .modal-content{padding-top:40rpx;}
.rule-body .modal-content view{font-size: 30rpx;color: #333;font-weight: bold;}
.rule-body .title{text-align: center;margin-bottom: 30rpx;}
.rule-body .modal-content text{font-size: 26rpx;line-height:42rpx;color: #333333;margin-bottom: 38rpx; display: block;}
.rule-body .modal-title{border-top:1px solid #f2f2f2;margin-top:80rpx;}
.rule-body .modal-title view{text-align: center;font-size: 32rpx;}
