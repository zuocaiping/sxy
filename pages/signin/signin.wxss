/* pages/list/list.wxss */
.m-btn button{border-radius: 60rpx;width: 535rpx;height: 88rpx;line-height: 88rpx;border:1rpx solid #e1e1e1;margin: 100rpx auto 0;text-align: center;font-size: 24rpx;color: #a9a9a9;position: relative;}
.m-btn image{position: absolute;width: 32rpx;height: 48rpx;left:70rpx;top:50%;margin-top:-24rpx;}
.m-btn.green button{background: #5acc82;color: #fff;border:1rpx solid #5acc82;font-size: 36rpx;}
.m-signin image:nth-child(1){width: 146rpx;height: 146rpx;margin: 0 auto;display: block;margin-top: 102rpx;}
.m-signin image:nth-child(2){width: 76rpx;height: 63rpx;margin: 226rpx auto 50rpx;display: block;}
.m-input{width:540rpx ;margin: 0 auto;position: relative;}
.m-input input{border-bottom: 1rpx solid #454545;height: 110rpx;margin: 0 auto 20rpx;padding-left: 52rpx;line-height: 110rpx; }
.m-input image{width:32rpx;height: 48rpx; position: absolute;left: 0;top: 50%;margin-top: -20rpx; }
.m-input.u-yzm image{width:44rpx;height: 44rpx; margin-top: -22rpx;}
.m-yzm{width: 160rpx;height: 60rpx; text-align: center; line-height: 60rpx;color: #fff;font-size: 22rpx;background: #444444; border-radius: 60rpx; display: inline-block;position: absolute;right: 0;top:50%;margin-top: -30rpx;}
.m-yzm-green{background: #5acc82;width: 160rpx;height: 60rpx; text-align: center; line-height: 60rpx;color: #fff;font-size: 22rpx; border-radius: 60rpx; display: inline-block;position: absolute;right: 0;top:50%;margin-top: -30rpx;}
.m-input input:placeholder{color: #444444;font-size: 32rpx;}
.m-conter{margin-top: 160rpx;}
.m-wx{text-align: center;margin-top: 137rpx;}
.m-wx button{border: none;background: none; position: inherit;font-size: 24rpx;color: #a9a9a9;}
.m-wx view{border:1rpx solid #8ed18c;width: 64rpx;height: 64rpx; display: block;border-radius: 60rpx;margin: 0 auto 30rpx;}
.m-wx view image{width: 46rpx;height: 33rpx;margin: 14rpx auto; display: block;}