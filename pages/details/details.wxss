.m-share1-list view{ flex: 1;font-size: 24rpx;color: #bfbfbf; text-align: center;}
.m-share1-list view image{width: 45rpx;height: 44rpx;margin-bottom:10rpx;}
.m-share1-list view text{display: block;}
.m-share1 .u-text{font-size: 24rpx;color: #b0afaf;}
.m-share1 .u-btn{border-radius: 30rpx;width: 224rpx;height: 65rpx; text-align: center; line-height: 65rpx;border:1px solid #e7e7e7;color: #777;font-size: 24rpx;margin: 38rpx auto;}
.m-share1 .u-btn button{font-size: 24rpx;margin: 0;padding: 0;background-color: transparent;height: 64rpx; line-height: 64rpx;}
.m-copyright button{margin: 0;padding: 0;display: inline;background-color: transparent;color:red;font-size:22rpx;text-decoration: underline;
}