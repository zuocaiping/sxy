.m-collection{width: 72rpx; text-align: center;}
.m-collection image{width:58rpx;height:58rpx;vertical-align:top;margin:8rpx 0 -6rpx;}
.m-collection text{display: inline-block;font-size: 24rpx; color: #4a576a;vertical-align:top;margin:8rpx 0 0 0rpx;}
.m-already image{width: 48rpx;height: 48rpx;left:15rpx;margin-top:-24rpx;}
.m-printing{width: 242rpx;height: 91rpx; color: #fff; background: #5acc82; font-size: 30rpx; font-weight: bold; text-align: center; line-height: 91rpx;border-radius: 50rpx;float: right;}
.m-printing-img{text-align: center;line-height: 88rpx; width: 88rpx;height: 88rpx; border-radius: 50rpx;position: fixed;right: 10rpx;top:50rpx; z-index: 50;    background:rgba(255,255,255,1);opacity:0.5;}
.m-printing-img image{width: 42rpx;height: 42rpx;margin: 22rpx auto;}
.m-printing-img text{width: 30rpx;height: 30rpx;border:1px solid #fff;background:#ef1d1d;border-radius: 50rpx;font-size: 22rpx;text-align: center;line-height: 30rpx; position: absolute;right:-5rpx;color: #fff;}
.scroll-view_H{width: 100%; white-space: nowrap;}
.b-desc.half{display: -webkit-box;word-break: break-all;-webkit-box-orient: vertical;-webkit-line-clamp:4;overflow: hidden;
text-overflow:ellipsis; font-size: 26rpx;}
.b-desc.showfull{display: block;overflow: hidden;white-space: nowrap;text-overflow:ellipsis;}
.m-copyright text{font-size: 22rpx; color: #b0afaf;}
.m-share1{border-bottom:1px solid #f0f0f0; text-align: center;margin: 0 35rpx;}
