page{padding-bottom:200rpx;}
.m-top{position: relative;padding-left:39rpx;margin-bottom:11rpx;line-height: 58rpx; }
.m-top .m-left{font-size: 29rpx;}
.m-top .m-left image{width:25rpx;height:28rpx;margin: 14rpx 10rpx 0 0; vertical-align: top;}
.u-bold{font-weight: bold;color: #ec2c2c;}
.u-receive{color: #ec2c2c;font-size: 25rpx;text-decoration: underline;}
.m-search{position: absolute;right:12rpx;font-size:24rpx; line-height: 58rpx; color:#999;width: 254rpx;border-radius:40rpx;height: 58rpx;background: #f7f7f7;top:0;padding:0 15rpx 0 60rpx;}
.m-search image{width: 24rpx;height: 24rpx;position: absolute;top:50%;margin-top:-12rpx;left:17rpx;}
.m-banner{height: 350rpx;margin-bottom:45rpx;}
.m-banner image{width: 100%;height: 350rpx;}
.m-data image{width: 145rpx;height: 183rpx; float: left;margin-right: 25rpx;}
.m-data{overflow: hidden;margin-bottom:82rpx;}
.m-data text{float: left;border-right:2rpx solid #cbcbcb;border-left:2rpx solid #cbcbcb;width: 25rpx;padding:0 20rpx; display: block;font-size: 28rpx;line-height: 46rpx; color: #989898;margin: 0 28rpx;}
.m-img{width: 999rpx;}
.m-title{font-size: 36rpx;font-weight: bold;color: #333;margin: 0 0 24rpx 35rpx;}
.m-title1{position: relative;padding-left: 22rpx;margin-bottom: 20rpx;}
.m-title1 .u-left{font-size: 30rpx; color: #989898;}
.m-title1 .u-left image{width: 46rpx;height: 46rpx; vertical-align: top;}
.u-more{font-size: 26rpx;color: #b9b9b9;display: inline-block; text-align: center;line-height: 42rpx;margin-left:10rpx;}
.u-more image{width: 26rpx;height: 28rpx;margin:8rpx 10rpx 0 0; vertical-align: top;}
.m-book{width:220rpx;height: 312rpx; float: left;margin:0 40rpx 0 0rpx; position: relative; }
.m-book image{width: 100%;height: 100%;  display: block;}
.m-book text{width:50rpx;height: 50rpx;border-radius: 50%;text-align: center;line-height: 50rpx;font-size: 16rpx;color: #fff; position: absolute;right:24rpx;top:-25rpx; display: block;background: #fdb73c;}
.m-recommend-con{ position: relative; display: flex;padding-top:108rpx;margin-right:38rpx;margin-left:38rpx; border-top:1px solid #f0f0f0;}
.m-recommend-con .u-title{position: absolute;top:35rpx;left:0;font-size: 30rpx; font-weight: bold;color: #333;width: 100%}
.m-con{flex: 1;margin-right: 30rpx;}
.m-con text{display: block;}
.u-name{font-size: 32rpx; color: #333;margin-bottom: 14rpx;}
.u-about{font-size: 26rpx; line-height: 42rpx;color: #b0afaf;margin-bottom: 20rpx;}
.u-about .more{color: #519b61;font-size: 24rpx;display: inline-block;}
.u-author{font-size: 24rpx; color: #262626;margin:20rpx 0 18rpx 14rpx;}
.u-author image{width: 53rpx;height: 49rpx; vertical-align: top;margin: -2rpx 0 0 -15rpx;}
.u-author text{font-weight: bold;color: #5acc82;}
.m-tab-title{display: flex;padding: 20rpx 0 10rpx;}
.m-tab-title view{flex: 1;text-align: center;font-size: 30rpx;color: #767676; line-height: 50rpx;position: relative;}
.m-tab-title .u-new{width: 30rpx;height: 30rpx; font-size: 20rpx; color: #fff;border:2rpx solid #fff;border-radius: 50%;display: inline-block;position: absolute;background: #ef1d1d;font-weight: bold;line-height: 30rpx;top:-10rpx;left:50%;margin-left: 5rpx;}
.m-tab-title image{width: 58rpx;height: 58rpx; display: block;margin: 0 auto;}

.m-broadcast{padding-left: 28rpx; overflow: hidden;margin-bottom:10rpx;}
.m-broadcast image{width: 250rpx;height: 380rpx;margin-right: 18rpx;}
/* .m-broadcast-con{width: 100000rpx;} */
.m-spot{margin: 10rpx 0 0 10rpx;}
.m-spot text{width: 10rpx;height: 10rpx;margin: 0 8rpx; display: inline-block;border-radius: 50%;background: #e7e6e6; vertical-align: top;}
.m-spot .u-current{width: 15rpx;height: 15rpx;background: #5acc82;margin-top: -2rpx;}
.m-data-about{padding: 0 38rpx 70rpx;font-size: 26rpx; line-height: 42rpx;color: #333333;}
.m-data-about .u-title{font-size: 30rpx; color: #333333; font-weight: bold;margin-bottom: 22rpx;}
.m-data-about .more{color: #519b61;font-size: 24rpx;}
.m-record{display: block; padding-bottom:73rpx;}
.m-recommend-con .u-title text{position: absolute;right:0;font-size: 24rpx;color: #519b61;font-weight: normal;}
.m-record-list{border-left:1px solid #e7e7e7; position: relative;padding:40rpx 0 50rpx 28rpx;}
.m-record-list:last-child{padding-bottom:0;}
.m-record-list .u-spot{width:14rpx;height: 14rpx;border-radius: 50%;display: inline-block;background: #e7e7e7; position: absolute;top:0;left:-7rpx; }
.m-record-list .u-time{font-size: 24rpx; color: #b0afaf; position: absolute;top:-5rpx;}
.m-record-list .u-text{font-size: 26rpx;color: #676767;line-height: 42rpx;}
.m-record-list .more{color: #519b61; display: inline-block;}
.m-comment{display: block;}
.m-comment .m-record-list{border-radius: 5rpx;background: #fafafa;border-left: none;margin-bottom:30rpx;padding: 70rpx 22rpx 24rpx;}
.m-comment .m-record-list .u-time{top:25rpx;}
.m-comment text.u-number{font-size: 24rpx; color: #b0afaf;position:initial;margin-left: 15rpx;}
.m-record-list .u-text1{font-size: 24rpx; color: #676767;line-height: 42rpx;display: block;}
.m-record-list .u-text1 text{display: block;width: 100%;}
.m-comment .m-information{padding-top:20rpx;}
.m-copyright{margin: 0 35rpx;}
.m-copyright view{font-size: 28rpx;  line-height: 40rpx; color: #b0afaf;}
.m-copyright .u-title{font-size:28rpx; color: #262626; font-weight: bold;margin-bottom:20rpx;}
.m-copyright  view text{color: #fe2121; text-decoration: underline;}
.m-copyright .u-title text{color: #fe2121;text-decoration: none;}
.m-fun{padding: 40rpx 35rpx 27rpx;overflow: hidden; position: fixed;width: 100%;background: #fff;bottom: 0;left: 0;box-sizing: border-box;}
.m-join image{width: 42rpx;height: 42rpx; position: absolute;left:25rpx;top:50%;margin-top:-21rpx;}
.m-join{ float: left;margin-right:12rpx; position: relative; border-radius: 50rpx;border:1px solid #4a576a;width: 237rpx; height: 91rpx; text-align: center; line-height: 91rpx;font-size: 30rpx; color: #4a576a;font-weight: bold;}
.m-join text{width:40rpx;
height:40rpx;
border:1px solid #fff;
background:#ef1d1d;
border-radius:50rpx;
font-size:22rpx;
text-align:center;
line-height:40rpx;
position:absolute;
right:-5rpx;top:-10rpx;
color:#fff;
}
.m-home{width: 56rpx; line-height: 20rpx; text-align: center;margin-right: 22rpx;font-size: 24rpx;color: #4a576a; display: inline-block; float: left;}
.m-home image{width: 64rpx;height: 64rpx;}
.m-share{width: 120rpx;height: 50rpx;border-radius: 30rpx;background: #f4f4f4; line-height: 50rpx; text-align: center;color: #4a576a;font-size: 24rpx; position: absolute;right:28rpx;top:50%;margin-top:-85rpx;}
.m-share button{ padding: 0; margin: 0;font-size: 24rpx;background-color: transparent; height: 50rpx;line-height: 50rpx;}
