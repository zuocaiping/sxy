.m-banner image{width: 100%;height: 350rpx;}
.m-data image{width: 145rpx;height: 183rpx; float: left;margin-right: 25rpx;}
.m-data{overflow: hidden;margin-bottom:82rpx; position: relative;}
.m-data text{float: left;border-right:2rpx solid #cbcbcb;border-left:2rpx solid #cbcbcb;width: 25rpx;padding:0 20rpx; display: block;font-size: 28rpx;line-height: 46rpx; color: #989898;margin: 0 28rpx;}
.m-img{width: 999rpx;}
.m-title{border-top: 1px solid #e6e6e6;  width: 255rpx; padding:0 45rpx; margin:0 auto 28rpx; text-align: center;display: block;}
.m-title text{font-size: 28rpx;font-weight: bold;color: #242424;background:#fff;margin-top:-25rpx; position: relative;z-index: 2; display: block;}
.m-title1{position: relative;padding-left: 32rpx;height: 70rpx;}
.m-title1 .u-left{font-size: 34rpx; color: #242424; position: relative;}
.m-title1 .u-left text{position: absolute;left: 0;color: #bcbbbb;font-size: 24rpx;top:-30rpx;}
.m-title1 .u-left image{width: 46rpx;height: 46rpx; vertical-align: top;margin-top:-4rpx;}
.m-specifications{color: #b0afaf;font-size: 24rpx;}
.m-specifications image{width:42rpx;height:42rpx;margin: -4rpx 0 0 20rpx; vertical-align: top;}
.u-more{border: 1px solid #eaeaea;font-size: 22rpx;color: #b9b9b9;border-radius: 30rpx;width: 88rpx;height: 42rpx;display: inline-block; text-align: center;line-height: 42rpx; position: absolute;right:35rpx;top:0;}
