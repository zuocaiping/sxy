/**首页css**/
.m-top{position:fixed; width: 100%;left: 0;top:0; z-index: 10;background: #fff; padding-left:39rpx;padding-bottom:11rpx;line-height: 58rpx; }
.m-top .m-left{font-size: 26rpx;}
.m-top .m-left image{width:25rpx;height:28rpx;margin: 16rpx 10rpx 0 0; vertical-align: top;}
.u-bold{font-weight: bold;color: #ec2c2c;}
.u-receive{color: #ec2c2c;font-size: 25rpx;text-decoration: underline;}
.m-search{position: absolute;right:46rpx;font-size:24rpx; line-height: 58rpx; color:#999;width: 254rpx;border-radius:40rpx;height: 58rpx;background: #f7f7f7;top:0;padding:0 15rpx 0 60rpx;}
.m-search image{width: 24rpx;height: 24rpx;position: absolute;top:50%;margin-top:-12rpx;left:17rpx;}
