.m-book{border-radius: 20rpx;width:300rpx;height: 280rpx; float: left;margin:0 30rpx 0 20rpx; position: relative; }
.m-book image{border-radius: 20rpx;width:300rpx;height: 280rpx; margin: 0rpx auto 0; display: block;}
.m-book text{width:50rpx;height: 50rpx;border-radius: 50%;text-align: center;line-height: 50rpx;font-size: 16rpx;color: #fff; position: absolute;right:24rpx;top:-25rpx; display: block;background: #fdb73c;}
.m-recommend-con{display: flex;margin-bottom:60rpx; }
.m-con{flex: 1;margin-right: 30rpx;}
.m-con text{display: block;}
.u-name{font-size: 32rpx; line-height: 36rpx; color: #333;margin-bottom: 18rpx;}
 .u-about{font-size: 26rpx; line-height: 32rpx;color: #b0afaf;margin-bottom: 20rpx;height: 63rpx;max-width: 339rpx;
 display:-webkit-box; float: left;
word-break:break-all;
-webkit-box-orient:vertical;
-webkit-line-clamp:2;
overflow:hidden;
text-overflow:ellipsis;
}
.u-author{font-size: 26rpx; color: #262626;}
.u-author image{width: 53rpx;height: 49rpx; vertical-align: top;margin: -2rpx 0 0 -15rpx;}
.u-link{width:130rpx;height: 53rpx;border-radius: 50rpx;background: #5acc82; text-align: center; line-height: 53rpx;color: #fff;font-size: 26rpx;font-weight: bold;}
.m-tab-title{ position: relative; display: flex;padding: 20rpx 0 10rpx; position: fixed;width: 100%;background: #fff;bottom: 0;left: 0;z-index: 1000;}
.m-tab-title view{flex: 1;text-align: center;font-size: 30rpx;color: #767676; line-height: 50rpx;position: relative;}
.m-tab-title .u-new{width: 30rpx;height: 30rpx; font-size: 20rpx; color: #fff;border:2rpx solid #fff;border-radius: 50%;display: inline-block;position: absolute;background: #ef1d1d;font-weight: bold;line-height: 30rpx;top:-10rpx;left:50%;margin-left: 5rpx;}
.m-tab-title image{width: 64rpx;height: 64rpx; display: block;margin: 0 auto;}
.m-tab-title .u-spot{width: 10rpx;margin:5rpx 0 0 15rpx;height:10rpx;background: red;border-radius:50%;display: inline-block;position: absolute;}
/*授权登录弹框*/
 
/*mask*/
.drawer_title image{width: 70rpx;height: 70rpx;vertical-align: top;margin:0 20rpx 0 0; }
.drawer_screen { 
 width: 100%; 
 height: 100%; 
 position: fixed; 
 top: 0; 
 left: 0; 
 z-index: 1000; 
 background: #000; 
 opacity: 0.5; 
 overflow: hidden; 
} 
  
/*content*/
.drawer_box { 
 width: 650rpx; 
 overflow: hidden; 
 position: fixed; 
 top: 50%; 
 left: 0; 
 z-index: 1001; 
 background: #FAFAFA; 
 margin: -150px 50rpx 0 50rpx; 
 border-radius: 10rpx; 
} 
  
.drawer_title{ 
 padding:100rpx 15px; 
 font: 24rpx "microsoft yahei"; 
 text-align: center; 
 line-height: 70rpx;
} 

  
.btn_ok{ 
 padding: 10px; 
 font: 24rpx "microsoft yahei"; 
 text-align: right; 
 border-top: 1px solid #E8E8EA; 
 color: #3CC51F; line-height: 60rpx;margin: 0 40rpx;
} 


