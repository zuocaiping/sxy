/* pages/list/list.wxss */
.m-top{position: relative;padding-left:39rpx;margin-bottom:11rpx;line-height: 58rpx;background: #fff; }
.m-top .m-left{font-size: 36rpx;color: #242424; }
.m-top .m-left image{width:18rpx;height:32rpx;margin: 14rpx 10rpx 0 0; vertical-align: top;}
.m-user{position: relative;padding:0 34rpx 0 167rpx;margin: 57rpx 0; line-height: 44rpx; }
.m-user image{width: 100rpx;height: 100rpx; border-radius: 50%;position: absolute;left: 36rpx;top: 0;}
.m-user .u-name{font-size: 36rpx; color: #000000;}
.m-user .u-id{font-size: 24rpx;color: #666666;margin-left: 14rpx;}
.m-user  view{font-size: 24rpx;color: #666666;}
.m-user  view text{font-weight: bold;color: #f24a5d;}
.m-btn{width: 98rpx;height: 50rpx; font-size: 24rpx; color: #000000; border-radius: 30rpx; line-height: 50rpx; text-align: center;border:1px solid #dbdbdb; position: absolute;right:34rpx;top:50%;margin-top: -25rpx;}
.m-list{font-size: 30rpx;color: #333333; font-weight: bold; position: relative;padding: 24rpx 29rpx 24rpx 35rpx;}
.m-list image{vertical-align: top;margin: -10rpx 30rpx 0 3rpx;width: 64rpx;height: 64rpx;}
image.u-arrow{width: 11rpx;height: 22rpx; position: absolute;right:0; top:50%;margin-top: -5rpx;}
.m-order{margin-top:24rpx;border-top:1px solid #f5f5f5;padding: 48rpx 29rpx 48rpx 35rpx;font-size: 30rpx; font-weight: bold;color: #333333; position: relative;}
.m-order .u-number{font-weight: normal;font-size: 22rpx;color: #808080;}
.m-order .u-arrow{ position: absolute;right:36rpx;top:50rpx;}
.m-block{display: flex;margin: 60rpx -29rpx 0 -35rpx;}
.m-block view{flex: 1; border-right:1px solid #f5f5f5; text-align: center;font-weight: normal; font-size: 28rpx;color: #808080;line-height: 48rpx; position: relative;}
.m-block view:last-child{border-right:none;}
.m-block image{display: block;margin:0 auto 10rpx;width: 64rpx;height: 64rpx;}
.m-block text{width: 32rpx;height: 32rpx; display: inline-block;border-radius: 50%;background: #eb624f; font-size: 22rpx; font-weight: bold;color: #fff; position: absolute;text-align: center;line-height: 32rpx;top:-15rpx;left: 50%;margin-left: 10rpx;}
.m-edition{font-size: 26rpx;color: #404040;margin: 158rpx 40rpx 30rpx; line-height: 36rpx; position: relative;}
.m-edition text{font-size:22rpx;color:#919191; display: block;}
.contact-btn{position: absolute; opacity: 0; width: 100%;height: 100%;left: 0;top: 0;}
.m-home{position: absolute;right:23rpx;text-align: center;font-size: 30rpx;color: #919191; line-height: 50rpx;top:-10rpx;}
.m-home image{width: 64rpx;height: 64rpx; display: block;margin: 0 auto;}
.m-home text{margin-top:-10rpx;}

